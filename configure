  __asm__ ("cpuid" : "+b" (ebx), "+a" (eax), "+c" (ecx), "=d" (edx));
  abcd[0] = eax; abcd[1] = ebx; abcd[2] = ecx; abcd[3] = edx;}
int
main (void)
{
uint32_t abcd[4];
 uint32_t bmi1_mask = (1 << 3);
 run_cpuid(1, 0, abcd);
 return ((abcd[/*EBX*/1] & bmi1_mask) == bmi1_mask) ? 0 : 9;
  ;
  return 0;
}
